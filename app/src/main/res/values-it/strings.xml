    <string name="settings_entry_default_contact_type">Tipo contatto predefinito</string>
    <string name="settings_entry_default_external_contact_account">Account contatti</string>
    <string name="settings_entry_default_contact_type_description">I contatti pubblici possono essere condivisi con altre app. I contatti segreti no.</string>
    <string name="settings_entry_default_external_contact_account_description">I contatti pubblici possono essere salvati localmente o in un account Google, ad esempio.</string>
    <string name="settings_entry_call_detection">Associa chiamate in entrata ai contatti</string>
    <string name="settings_entry_call_detection_description">Confronta le chiamate entranti con i contatti salvati e mostra una notifica.</string>
